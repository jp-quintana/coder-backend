<main>
      <h1 class="page-title">Productos</h1>
      {{#if hasProducts}}

      <table class="productos">
        <tr>
          <th>Titulo</th>
          <th>Precio</th>
          <th>Thumbnail URL</th>
        </tr>
        {{#each products}}
        <tr>
          <td>{{this.title}}</td>
          <td>{{this.price}}</td>
          <td>{{this.thumbnail}}</td>
        </tr>
        {{/each}}
      </table>
      {{else}}
      <p>No se agregaron productos todavia!</p>
      {{/if}}
    </main>